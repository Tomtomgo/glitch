<html>
<head>

  <link rel="stylesheet" href="http://code.jquery.com/ui/1.10.1/themes/base/jquery-ui.css" />
  <script src="http://code.jquery.com/jquery-1.9.1.js"></script>
  <script src="http://code.jquery.com/ui/1.10.1/jquery-ui.js"></script>

  <%= javascript_include_tag 'underscore-min' %>
  <%= javascript_include_tag 'youtube-video' %>
  <%= javascript_include_tag 'tuna' %>
  <%= javascript_include_tag 'sounds' %>
  <%= javascript_include_tag 'glitch' %>


</head>
<body>

  <canvas id="canv" width="100%" height="100%"></canvas>
  <canvas id="data_canv" style="display: none;" width="100%" height="100%"></canvas>
  <video width="100%" height="100%" style="display: none;"></video>

  <div id="start" class="box">
    <div class="predefined">AyJfHU4GoOQ</div>
    <input type="text" id="video_source" value="N4_jCeQuqqM"></input>

    <div id="gogo">Go!</div>
  </div>

  <div id="fps"></div>

  <div id="controls" class="box">
    <label for="low_threshold">Low threshold</label>
    <div id="low_threshold"></div>
    <label for="mid_threshold">Mid threshold</label>
    <div id="mid_threshold"></div>
    <label for="high_threshold">High threshold</label>
    <div id="high_threshold"></div>
  </div>

</body>
</html>